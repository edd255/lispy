;==== Fibonacci Test ==========================================================
(load "tests/test.lspy")

(fn {day-name x} {
  case x
    {0 "Monday"}
    {1 "Tuesday"}
    {2 "Wednesday"}
    {3 "Thursday"}
    {4 "Friday"}
    {5 "Saturday"}
    {6 "Sunday"}
})

(test "Monday" (day-name 0))
(test "Tuesday" (day-name 1))
(test "Wednesday" (day-name 2))
(test "Thursday" (day-name 3))
(test "Friday" (day-name 4))
(test "Saturday" (day-name 5))
(test "Sunday" (day-name 6))

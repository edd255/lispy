;==== nth Test ================================================================
(fn {test got exp} {
    testhelper (== exp got) exp got
})

(test (tail "Hello, World!") "ello, World!") "ello, World!" (tail "Hello, World!")
(test (tail "ok") "k")
(test (head "Hello, World!") "H")
(test (head "ok") "o")

(test (head {1 2 3 4}) {1})
(test (tail {1 2 3 4}) {2 3 4})

(test (head {1}) {1})
(test (tail {1}) {})
